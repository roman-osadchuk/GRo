{
  "name": "GRo_api",
  "version": "1.0.0",
  "main": "index.js",
  "description": "API for GRo",
  "license": "MIT",
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "devserver": "nodemon --exec babel-node -- src/index.js",
